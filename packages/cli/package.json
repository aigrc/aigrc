{
  "name": "@aigrc/cli",
  "version": "0.1.0",
  "description": "CLI for AIGRC - AI Governance, Risk, Compliance",
  "type": "module",
  "bin": {
    "aigrc": "./dist/bin/aigrc.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/bin/aigrc.js",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@aigrc/core": "workspace:*",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.0",
    "inquirer": "^9.3.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  }
}
