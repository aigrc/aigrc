{
  "name": "@aigrc/cli",
  "version": "0.2.0",
  "description": "CLI for AIGRC - AI Governance, Risk, Compliance",
  "license": "Apache-2.0",
  "author": "AIGRC <contact@aigrc.dev>",
  "homepage": "https://github.com/aigrc/aigrc#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aigrc/aigrc.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/aigrc/aigrc/issues"
  },
  "keywords": [
    "aigrc",
    "ai-governance",
    "risk-management",
    "compliance",
    "eu-ai-act",
    "nist-ai-rmf",
    "iso-42001",
    "cli",
    "command-line"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "bin": {
    "aigrc": "./dist/aigrc.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/aigrc.js",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@aigrc/core": "^0.2.0",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.0",
    "inquirer": "^9.3.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  }
}
