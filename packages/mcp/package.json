{
  "name": "@aigrc/mcp",
  "version": "2.0.0",
  "description": "AIGRC MCP Server - AI Governance via Model Context Protocol",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "aigrc-mcp": "./dist/bin/aigrc-mcp.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "start": "node dist/bin/aigrc-mcp.js"
  },
  "keywords": [
    "aigrc",
    "mcp",
    "model-context-protocol",
    "ai-governance",
    "compliance",
    "eu-ai-act"
  ],
  "author": "AIGRC",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@aigrc/core": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "glob": "^10.3.10",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
