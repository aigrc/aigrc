{
  "name": "@aigrc/core",
  "version": "3.1.0",
  "description": "Core logic for AIGRC - schemas, validation, risk classification",
  "license": "Apache-2.0",
  "author": "AIGRC <contact@aigrc.dev>",
  "homepage": "https://github.com/aigrc/aigrc#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aigrc/aigrc.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/aigrc/aigrc/issues"
  },
  "keywords": [
    "aigrc",
    "ai-governance",
    "risk-management",
    "compliance",
    "eu-ai-act",
    "nist-ai-rmf",
    "iso-42001",
    "schemas",
    "validation",
    "zod"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.mjs",
      "require": "./dist/schemas/index.js"
    },
    "./air": {
      "types": "./dist/air/index.d.ts",
      "import": "./dist/air/index.mjs",
      "require": "./dist/air/index.js"
    },
    "./governance-lock": {
      "types": "./dist/governance-lock/index.d.ts",
      "import": "./dist/governance-lock/index.mjs",
      "require": "./dist/governance-lock/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "yaml": "^2.3.4",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}